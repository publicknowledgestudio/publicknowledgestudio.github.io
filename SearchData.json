---
permalink: /SearchData.json
---

{
  {%- assign i = 0 -%}
  {%- for project in site.projects -%}
    {%- if project.feed == "show" -%}

    {%- if i > 0 -%},{%- endif -%}
    "{{ i }}": {
       "doc":  {{ project.title | strip_html | escape | jsonify }},
       "title": {{ project.title | strip_html | escape | jsonify }},
       "content": {{ project.content | replace: '</h', ' . </h' | replace: '<hr', ' . <hr' | replace: '</p', ' . </p' | replace: '<ul', ' . <ul' | replace: '</ul', ' . </ul' | replace: '<ol', ' . <ol' | replace: '</ol', ' . </ol' | replace: '</tr', ' . </tr' | replace: '<li', ' | <li' | replace: '</li', ' | </li' | replace: '</td', ' | </td' | replace: '<td', ' | <td' | replace: '</th', ' | </th' | replace: '<th', ' | <th' | strip_html | remove: 'Table of contents' | normalize_whitespace | replace: '. . .', '.' | replace: '. .', '.' | replace: '| |', '|' | append: ' ' | strip_html | strip_newline | strip | escape | jsonify }},
       "url": "{{ site.baseurl }}{{ project.url }}"
    }{%- assign i = i | plus: 1 -%}

    {%- endif -%}
  {% endfor %}

  {%- for post in site.blog -%}
    {%- if post.feed == "show" -%}

    {%- if i > 0 -%},{%- endif -%}
    "{{ i }}": {
       "doc":  {{ post.title | strip_html | escape | jsonify }},
       "title": {{ post.title | strip_html | escape | jsonify }},
       "content": {{ post.content | replace: '</h', ' . </h' | replace: '<hr', ' . <hr' | replace: '</p', ' . </p' | replace: '<ul', ' . <ul' | replace: '</ul', ' . </ul' | replace: '<ol', ' . <ol' | replace: '</ol', ' . </ol' | replace: '</tr', ' . </tr' | replace: '<li', ' | <li' | replace: '</li', ' | </li' | replace: '</td', ' | </td' | replace: '<td', ' | <td' | replace: '</th', ' | </th' | replace: '<th', ' | <th' | strip_html | remove: 'Table of contents' | normalize_whitespace | replace: '. . .', '.' | replace: '. .', '.' | replace: '| |', '|' | append: ' ' | strip_html | strip_newline | strip | escape | jsonify }},
       "url": "{{ site.baseurl }}{{ post.url }}"
    }{%- assign i = i | plus: 1 -%}

    {%- endif -%}
  {% endfor %}

  {%- for member in site.team -%}

    {%- if i > 0 -%},{%- endif -%}
    "{{ i }}": {
       "doc":  {{ member.title | strip_html | escape | jsonify }},
       "title": {{ member.title | strip_html | escape | jsonify }},
       "content": {{ member.content | replace: '</h', ' . </h' | replace: '<hr', ' . <hr' | replace: '</p', ' . </p' | replace: '<ul', ' . <ul' | replace: '</ul', ' . </ul' | replace: '<ol', ' . <ol' | replace: '</ol', ' . </ol' | replace: '</tr', ' . </tr' | replace: '<li', ' | <li' | replace: '</li', ' | </li' | replace: '</td', ' | </td' | replace: '<td', ' | <td' | replace: '</th', ' | </th' | replace: '<th', ' | <th' | strip_html | remove: 'Table of contents' | normalize_whitespace | replace: '. . .', '.' | replace: '. .', '.' | replace: '| |', '|' | append: ' ' | strip_html | strip_newline | strip | escape | jsonify }},
       "url": "{{ site.baseurl }}{{ member.url }}"
    }{%- assign i = i | plus: 1 -%}

  {% endfor %}
}
